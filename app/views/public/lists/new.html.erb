<div class = "container">
  <div class = "row">
    <div class = "col-sm-12">
      <h1>投稿フォーム</h1>
      <%= form_with model: @list, local:true do |f| %>
      <%= f.label :日付選択 %>
      <%= f.date_field :date %>
      <%= f.label :一つ目の項目 %>
      <%= f.text_field :first_item %>
      <%= f.label :二つ目の項目 %>
      <%= f.text_field :second_item %>
      <%= f.label :三つ目の項目 %>
      <%= f.text_field :third_item %>
      <%= f.submit "投稿" %>
      <% end %>
    </div>
  </div>
</div>